{
  "name": "jira-branch-creator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:main\" \"npm:dev:content\"",
    "dev:main": "vite build --watch --mode development",
    "dev:content": "vite build --config vite.config.content.ts --watch --mode development",
    "build": "tsc -b && vite build && vite build --config vite.config.content.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "pino": "^9.4.0",
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "chrome-types": "^0.1.389",
    "concurrently": "^9.1.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-solid": "^2.11.10"
  }
}
